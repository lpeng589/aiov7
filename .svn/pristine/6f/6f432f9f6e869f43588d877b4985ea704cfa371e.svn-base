.add {
width:100%;
text-align:right;
float:right; color:#FF0000; font-weight:bolder; font-size:12px;}
.add a:link {
	text-decoration: none;
	color:#ff0000;
}
.add a:visited {
	text-decoration: none;
	color:#ff0000;
}
.add a:hover {
	text-decoration:underline;
	color:#ff0000
}
.add a:active {
	text-decoration: none;
	color:#ff0000
}

