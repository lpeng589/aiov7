.upgrade_mtop{
	margin-top:30px;
}
#contentleft {
	background: url(../images/reg/contentcenter.jpg) repeat-x; width: 745px; height: auto
}
#contentright {
	min-height: 400px; background: url(../images/reg/contentright.jpg) no-repeat right top; float: right; width: 745px
}
#contentmain {
	min-height: 300px; background: url(../images/reg/contentleft.jpg) no-repeat; float: left; width: 731px; padding-top: 0px; height: 300px;
}
#contentmain p {
	padding-right: 30px; padding-left: 30px; line-height: 20px; text-align: left
}
#pagetitle {
	width: 640px; border-bottom: #ccc 1px solid; height: 22px; 
}
#pagetitle span {
	font-weight: bold; font-size: 12px; float: left; color: #5e5e5e; 
}
#pagetitle p {
	margin:0px;
	padding:0px;
	line-height:20px;
}
#pagetitle div {
	float: right
}
#pagetitle h1 {
	padding-right: 5px; padding-left: 15px; font-size: 12px; float: left; padding-bottom: 0px; padding-top: 0px
}
#pagetitle h2 {
	padding-right: 5px; padding-left: 15px; font-size: 12px; float: left; padding-bottom: 0px; padding-top: 0px
}
.step3 {
	background: url(../images/reg/arw.jpg) no-repeat; float: left; width: 70px; height: 14px
}
.step1 {
	color: #bc8e6c
}
.step21 {
	color: #ff0000
}
.step21 a:link {
	text-decoration: none;
	color:#ff0000;
}
#content {
	margin-left: 50px; padding-top: 40px; text-align: left; 
}


#bodyUl {
	width:650px;padding-bottom:20px!important;padding-bottom:0px;
	font-size:14px;
	line-height:25px;
	color:#666;
	height:200px;
	margin-top: 30px; text-align: left;border-bottom: #ccc 1px dotted;
}
#bodyUl span {
	padding-left:5px;
	padding-right:5px;
	font-weight: bold; font-size: 12px; color: #ff3333;
	vertical-align:middle;
}
#bodyul ul {
	margin:0px;
	padding:0px;
	list-style-type: none
}
#bodyul li {
	margin-bottom: 10px; width: 650px; height: 25px
}

