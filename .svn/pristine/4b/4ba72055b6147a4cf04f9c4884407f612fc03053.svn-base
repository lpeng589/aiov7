body{font-size:12px; font-family:Tahoma, Geneva, sans-serif; padding:0px; margin:0px;}
.google_top{position:absolute; top:0; width:100%; left:0; text-align:center; padding-top:10px;}
.google_right{position:fixed; bottom:0; right:0; width:272px; _position:absolute; _top: expression(documentElement.scrollTop + 42); overflow:visible; text-align:center;}
#container{width:660px; margin:0px auto; margin-top:110px; text-align:center; margin-bottom:32px;}
#gallery{padding:30px; background:#e1eef5;}
#descriptions{position:relative; height:50px; background:#EEE;width:640px; padding:10px; overflow:hidden;}
#descriptions .ad-image-description{position: absolute;}
#descriptions .ad-image-description .ad-description-title{display: block;}
p,p a{font-size:12px;color#666;}
