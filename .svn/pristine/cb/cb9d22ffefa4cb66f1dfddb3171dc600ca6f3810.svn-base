/*css reset*/

body , div,td { font-size: 12px;line-height: 22px; font-family: arial, helvetica,"宋体", sans-serif;}
body {margin:0px; padding:0px;background-color:#fff; }
div,form,img,ul,ol,li,dl,dt,dd ,em{margin:0px; padding:0px; border:0px; list-style:none; }
p,h1,h2,h3,h4,h5,h6 {margin:0px; padding:0px; font-size:12px; font-weight:normal;}
INPUT { font-size: 12px; font-family: arial, helvetica, sans-serif;padding:2px}
SELECT {font-size: 12px; font-family:arial, helvetica, sans-serif;padding:2px}
ul,li {list-style-type: none;} 

input[type=checkbox] { vertical-align: middle;  }
label { vertical-align: middle; } 

 

/*全局公共样式*/
textarea{resize:none;} /*hack for chrome, disable chrome resizes textarea*/
a { color:#0179bb;text-decoration: none;}
a:link { color: #0179bb;} 
a:hover { color: #ff3300; text-decoration: none;} 


.cf{zoom:1;}
.cf:after{content:'.';display:block;visibility:hidden;clear:both;height:0px;}

.clear {clear:both;display:block;height:0;overflow:hidden;margin:0px 6px;} 

/* bg */
.alldot,.btnbg,#head ,.main_search .col ul li.sel ,.main_search .col .inpsearch,.main_search .col .inpsearch_btn ,.maintablearea .bd .maintop ,.maintablearea .bd li.col_name,.zk ,.pagerightbtn ,
.op ,.op .a,.op .c,.sbtn,.sbtn .a,.sbtn .c
{background:transparent url(/style/images/client/bg.gif) no-repeat scroll 1000px 1000px ;}
.all_icon 
{background:transparent url(../images/icon.gif) no-repeat scroll 1000px 1000px ;}
.navtop
{background:transparent url(../images/bbg.gif) no-repeat scroll 1000px 1000px ;}

