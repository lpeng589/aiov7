otalRemain as PreReceiveTotalRemain1,vbCrLf    (case when '@Sess:Local'='zh_CN' then '预收期初明细' when '@Sess:Local'='zh_TW' then '預收期初明細' when '@Sess:Local'='en' then 'Pre-receive Initial Detail' end) as PreReceiveListvbCrLffrom tblCompanyvbCrLfouter apply (select 			vbCrLf					isnull(sum(BeginAmount),0) as PreReceiveBegin,vbCrLf					isnull(sum(TotalDebit),0) as PreReceiveTotalDebit,vbCrLf					isnull(sum(TotalLend),0) as PreReceiveTotalLend,vbCrLf					isnull(sum(TotalRemain),0) as PreReceiveTotalRemain,vbCrLf					isnull(sum(CurBeginAmount),0) as CurPreReceiveBegin,vbCrLf					isnull(sum(CurTotalDebit),0) as CurPreReceiveTotalDebit,vbCrLf					isnull(sum(CurTotalLend),0) as CurPreReceiveTotalLend,vbCrLf					isnull(sum(CurTotalRemain),0) as CurPreReceiveTotalRemainvbCrLf					from tblCompanybeginningDet x ,tblCompanybeginningvbCrLfWHERE tblCompanybeginning.BeginningType ='1' ANDvbCrLf				 x.SCompanyID=tblCompany.SCompanyIDvbCrLf					and x.CompanyCode like tblCompany.classCode+'%' AND tblCompanybeginning.id = x.f_ref) as tblCompanyTotalvbCrLfwhere 1=1 vbCrLf      and (tblCompany.classCode =?)vbCrLf      and (tblCompany.ClientFlag =2 or tblCompany.ClientFlag=3)vbCrLf      and (tblCompany.SCompanyID like '@Sess:SCompanyID'+'%')vbCrLf      and (tblCompanyTotal.PreReceiveTotalRemain &lt;&gt; ?)vbCrLfvbCrLforder by vbCrLf   tblCompany.classCode ,vbCrLf   tblCompany.ComFullName vbCrLfvbCrLf"/><charts/></ReportDataSource>
