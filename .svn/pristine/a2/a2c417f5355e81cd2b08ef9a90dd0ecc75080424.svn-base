urnStockDet.f_ref and tblSalesReturnStockDet.id=(select top 1 a.id from tblSalesReturnStockDet a where a.f_ref=tblSalesReturnStock.id))vbCrLf left join tblDepartment on tblDepartment.classCode=tblSalesReturnStock.DepartmentCodevbCrLf left join tblEmployee tblEmployee1 on tblEmployee1.id=tblSalesReturnStock.EmployeeID vbCrLf left join tblEmployee tblEmployee2 on tblEmployee2.id=tblSalesReturnStock.createByvbCrLf left join tblSalesOrder on tblSalesOrder.id=(case when len(isnull(tblSalesReturnStock.SaleOrderID,''))=0 then tblSalesReturnStockDet.SalesOrderID else tblSalesReturnStock.SaleOrderID end)vbCrLf left join tblSalesOutStock on tblSalesOutStock.id=(case when len(isnull(tblSalesReturnStock.SalesOutStockID,''))=0 then tblSalesReturnStockDet.SalesOutStockID else tblSalesReturnStock.SalesOutStockID end)vbCrLfwhere 1=1 vbCrLf      and (tblSalesReturnStock.BillDate &gt;= ?)vbCrLf      and (tblSalesReturnStock.BillDate &lt;= ?)vbCrLf      and (tblSalesReturnStock.BillNo like '%'+?+'%')vbCrLf      and (tblCompany.ComFullName like '%'+?+'%')vbCrLf      and (tblCompany.ComName like '%'+?+'%')vbCrLf      and (tblSalesReturnStock.TotalTaxAmount = ?)vbCrLf      and (tblDepartment.DeptFullName like '%'+?+'%')vbCrLf      and (tblEmployee1.EmpFullName like '%'+?+'%')vbCrLf      and (tblSalesOutStock.BillNo like '%'+?+'%')vbCrLf      and (tblSalesOrder.BillNo like '%'+?+'%')vbCrLf      and (tblEmployee2.EmpFullName like '%'+?+'%')vbCrLfvbCrLforder by vbCrLf   tblSalesReturnStock.BillDate desc,vbCrLf   tblSalesReturnStock.BillNo descvbCrLfvbCrLf"/><charts/></ReportDataSource>
