/* CSS Document */
body{
	width:100%;
	margin:0 auto;
	font-size:12px;
	overflow:hidden;
}
em{font-style:normal;}
a:link {
	text-decoration: none;
	color:#333333;
}
a:visited {
	text-decoration: none;
	color:#333333;
}
a:hover {
	text-decoration:underline;
	color:#000;
}
a:active {
	text-decoration: none;
	color:#000;
}
img {
	border:0px;
}
li {
	list-style:none;
}
input,select,button{outline:0;}
.login_all {
	width:100%;
	padding-top:100px;
}
.login_min {
	width:100%;
}

.login_minleft {
	float:left;
	width:60%;
	border:1px #FF0000 solid;
	height:340px;
}
.login_in {
	float:left;
	width:300px;
	height:340px;
	border:1px #0000ff solid;
}
.login_minright {
	float:right;
	height:340px;
	width:100px;
	border:1px #cccccc solid;
}
.right_bg {
	padding-left:120px;
}
.login_minbg {
	background:url(../images/login_bp/login_minbg.gif) repeat-y;
}
.koron_info {
	vertical-align:baseline;
	padding:0px;
	margin:0px;
	font-size:12px;
	height:30px;
	color:#333333;
	height:30px;
	padding-top:8px;
	text-indent:95px;
	background:url(../images/login_bp/koron_info.gif) no-repeat;
}
.koron_info span {
	vertical-align:middle;
}
.koron_info li {
	float:left;
	text-align:left;
}
.login_into {
	width:300px;
	padding:0;
	margin:-30px 0 0 0;
	overflow:hidden;
}
.login_into li {
	height:30px;
	line-height:180%;
	color:#666666;
	text-align:right;
	font-weight:600;
	font-size:14px;
	list-style:none;
	padding:0 40px 0 0;
	margin:0 0 5px 0;
}
.login_into span {
	width:140px;
	text-align:left;
}
.login_into select {
	width:140px;
}
.login_user {
	padding:0px;
	margin:0px;
}
.login_user li {
	text-align:center;
	height:31px;

}
.login_user button {
	float:left;
	width:132px;
	height:31px;
	line-height:31px;
	border:0px;
	background:url(../images/login_bp/login_buttonbg.gif) no-repeat;
	font-size:16px;
	color:#666666;
	font-weight:600;
	text-align:center;
	cursor:pointer;
}
.login_user .su_dv{display:inline-block;float:left;font-family:微软雅黑;font-size:12px;margin:10px 0 0 40px;overflow:hidden;width:90px;}
.login_user .su_dv em{line-height:17px;float:left;margin:0 0 0 3px;}

.left_bp {
	width:287px;
	height:184px;
	margin:0px;
	padding:0px;
	background:url(../images/login_bp/left_bp.gif) no-repeat;
}