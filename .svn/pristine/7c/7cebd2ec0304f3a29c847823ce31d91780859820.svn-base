upName="" fieldIdentity="Amount" analysis="0"><Displays><Display localStr="zh_CN" display="预付余额"/><Display localStr="en" display="Pre-pay Remaining"/><Display localStr="zh_TW" display="預付餘額"/></Displays><groupNames/></field><field fieldname="(case when '@Sess:Local'='zh_CN' then '预付期初明细' when '@Sess:Local'='zh_TW' then '預付期初明細' when '@Sess:Local'='en' then 'Pre-pay Initial Detail' end)" AsfieldName="PrePayList" groupByFlag="0" displayFlag="1" condition="" conditionJoin="" orderbyFlag="" display="预付期初明细" fieldType="2" inputType="0" popUpName="" width="90" classCode="1" linkAdd="ReportDataAction.do?reportNumber=ReportCompanybeginning&amp;CompanyCode=@ValueofDB:classCode&amp;hide_CompanyCode=@ValueofDB:hidecode&amp;tblCompany.ComFullName=@ValueofDB:ComFullName&amp;BeginningType=3" order="20" isStat="0" defaultValue="" fieldSysType="" fixColName="1" isNull="0" planarField="0" subSQL="" repeatNotShow="0" zeroDisplay="0" seriesCol="" seriesNums="" crossField="" groupName="" fieldIdentity="" analysis="0"><Displays><Display localStr="zh_CN" display="预付期初明细"/><Display localStr="en" display="Prepayment detail"/><Display localStr="zh_TW" display="預付期初明細"/></Displays><groupNames/></field></fieldInfo><SQL text="select vbCrLf    tblCompany.ComNumber as ComNumber,vbCrLf    tblCompany.ComFullName as ComFullName,vbCrLf    tblCompanyTotal.PrePayBegin as PrePayBegin,vbCrLf    tblCompanyTotal.PrePayTotalDebit as PrePayTotalDebit,vbCrLf    tblCompanyTotal.PrePayTotalLend as PrePayTotalLend,vbCrLf    tblCompanyTotal.PrePayTotalRemain as PrePayTotalRemain,vbCrLf    tblCompany.ClientFlag as ClientFlag,vbCrLf    tblCompany.SCompanyID as SCompanyID,vbCrLf    tblCompany.classCode+'#;#'+convert(varchar(2),tblCompany.isCatalog)+'#;##;#' as hidecode,vbCrLf    tblCompany.classCode as classCode,vbCrLf    tblCompany.isCatalog as isCatalog,vbCrLf    tblCompanyTotal.PrePayTotalRemain as PrePayTotalRemain1,vbCrLf    (case when '@Sess:Local'='zh_CN' then '预付期初明细' when '@Sess:Local'='zh_TW' then '預付期初明細' when '@Sess:Local'='en' then 'Pre-pay Initial Detail' end) as PrePayListvbCrLffrom tblCompanyvbCrLfouter apply (	select 			vbCrLf					isnull(sum(BeginAmount),0) as PrePayBegin,vbCrLf					isnull(sum(TotalDebit),0) as PrePayTotalDebit,vbCrLf					isnull(sum(TotalLend),0) as PrePayTotalLend,vbCrLf					isnull(sum(TotalRemain),0) as PrePayTotalRemain,vbCrLf					isnull(sum(CurBeginAmount),0) as CurPrePayBegin,vbCrLf					isnull(sum(CurTotalDebit),0) as CurPrePayTotalDebit,vbCrLf					isnull(sum(CurTotalLend),0) as CurPrePayTotalLend,vbCrLf					isnull(sum(CurTotalRemain),0) as CurPrePayTotalRemainvbCrLf					from tblCompanybeginningDet x ,tblCompanybeginningvbCrLfWHERE tblCompanybeginning.BeginningType ='3' ANDvbCrLf				 x.SCompanyID=tblCompany.SCompanyIDvbCrLf					and x.CompanyCode like tblCompany.classCode+'%' AND tblCompanybeginning.id = x.f_ref) as tblCompanyTotalvbCrLfwhere 1=1 vbCrLf      and (tblCompany.classCode =?)vbCrLf      and (tblCompany.ClientFlag =1 or tblCompany.ClientFlag=3)vbCrLf      and (tblCompany.SCompanyID like '@Sess:SCompanyID'+'%')vbCrLf      and (tblCompanyTotal.PrePayTotalRemain &lt;&gt; ?)vbCrLfvbCrLforder by vbCrLf   tblCompany.classCode ,vbCrLf   tblCompany.ComFullName vbCrLfvbCrLf"/><charts/></ReportDataSource>
