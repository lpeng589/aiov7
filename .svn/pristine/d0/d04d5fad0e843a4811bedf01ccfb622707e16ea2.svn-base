body,div,iframe,p,em,input,select,button,a,img,ul,ol,li,h1,h2,h3,h4,h5,h6,table,thead,tbody,tfoot,dl,dt,dd,form,textarea{margin:0;padding:0;}
img{border:0px;}

body {background-color:#fff;font-size: 12px;line-height:22px;overflow-x:hidden;font-family: "Microsoft Yahei", Helvetica, verdana,arial, helvetica,"微软雅黑", sans-serif;}
em{font-style:normal;}
a:link {text-decoration: none;color:#333333;}
a:visited {text-decoration: none;color:#333333;}
a:hover {text-decoration: none;color:#0060FF}
a:active {text-decoration: none;	color:#0060FF}
.top{background:url(../images/home/logo_bg.jpg) no-repeat;background-color:#1249c5;width:100%;height:65px;}
.logo{width:447px;height:42px;float:left;text-align:left;padding-top:18px;padding-left:10px;color:#97B568;font-size:18px;font-weight:900;}
/*-- menu top--*/
#menuList{height:66px;position:relative;}
#menuList .lavaLampWithImage{position:absolute;bottom:29px;z-index:51;width:400px;}
.menu_a {width:100%;position:absolute;bottom:8px;left:0;text-align:right;}

