/** body.jsp 顶部菜单 **/
ul.jd_menu{float:left;box-shadow:1px 1px #a1a1a1;border-radius:0 3px 3px 3px;display:inline-block;position: relative;height:24px;background:url(/style/images/body2/nav_ss.jpg) repeat-x;background-position:0 0;}

ul.jd_menu ul,ul.jd_menu_vertical ul {display: none;}
ul.jd_menu li {float: left;}
ul.jd_menu li.bd_li{background:url(/style/images/body2/whiteLine.jpg) no-repeat 77px 4px;}

ul.jd_menu li span.menu_s{float:none;color:#fff;line-height:25px;}

ul.jd_menu_vertical {width: 300px;}
ul.jd_menu_vertical li {float: none;}
ul.jd_menu ul ul,ul.jd_menu_vertical ul {position: absolute;display: none;margin:0 0 0 3px;padding:0px;z-index: 10000;}
ul.jd_menu ul li,ul.jd_menu_vertical ul li {float:none;margin:0 2px 0 2px;}
ul.jd_menu_vertical,ul.jd_menu_vertical ul {color:#000000;height: 20px;}
ul.jd_menu ul {width:145px;}
ul.erp_bg ul{background:#689153;}
ul.oa_bg ul{background:#2F5554;}
ul.crm_bg ul{background:#5E4567;}
ul.hr_bg ul{background:#AC532D;}


ul.jd_menu_vertical {height: auto}

ul.jd_menu ul ul{}

ul.jd_menu li.top_link_1 {font-family: sans-serif;font-size: 12px;color: #FFFFFF;line-height: 29px;margin: 0px;cursor: pointer;width:70px;}
ul.jd_menu li.top_link_1 span{float:none;line-height:13px;font-family:微软雅黑;}

ul.jd_menu li li {border-bottom:1px #C4C4C4 solid;width:130px;color:#fff;;padding:5px 0 5px 15px;height: 13px;text-align:left;vertical-align:top;margin:0;line-height:16px;}
ul.jd_menu li li img{}


ul.jd_menu li.jdm_hover,ul.jd_menu li.jdm_active {position:relative;}
ul.jd_menu li li.jdm_hover,ul.jd_menu li li.jdm_active{line-height:16px;}

ul.erp_bg li li.jdm_hover,ul.erp_bg li li.jdm_active{background:#8FC950;}
ul.oa_bg li li.jdm_hover,ul.oa_bg li li.jdm_active{background:#5DAAA8;}
ul.crm_bg li li.jdm_hover,ul.crm_bg li li.jdm_active{background:#BB8ACD;}
ul.hr_bg li li.jdm_hover,ul.hr_bg li li.jdm_active{background:#DE6C3A;}

ul.jd_menu li.jdm_hover span.menu_s{color:#000;}
ul.jd_menu li.jdm_hover span.menu_s_i{display:inline-block;width:auto;}

