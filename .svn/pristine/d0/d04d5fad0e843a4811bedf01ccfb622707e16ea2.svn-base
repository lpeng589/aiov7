.lavaLampWithImage {
	position: relative;
	height: 21px;
	padding:0 0 0 0 ;
	margin:0 0 0 0;		
}

.lavaLampWithImage .lava_span{position:absolute;width:55px;height:20px;background-image:url(/style/images/body2/nav_wl.png);display:inline-block;text-align:center;line-height:22px;}
.lavaLampWithImage .lava_span a{color:#fff;}

.lavaLampWithImage .bg_1{background-position:0 0;left:0;bottom:3px;}
.lavaLampWithImage .bg_2{background-position:165px 0;left:58px;bottom:3px;}
.lavaLampWithImage .bg_3{background-position:110px 0;left:116px;bottom:3px;}
.lavaLampWithImage .bg_4{background-position:55px 0;left:174px;bottom:3px;}

.lavaLampWithImage .bg_1_c{bottom:1px;height:27px;line-height:27px;color:#fff;}

.lavaLampWithImage li {
	float: left;
	list-style: none;
}
.lavaLampWithImage li.back {
	background: url("../images/home/lava.gif") no-repeat right -13px;
	width: 7px; height: 13px;
	z-index: 8;
	position: absolute; 
}
.lavaLampWithImage li.back .left {
	background: url("../images/home/lava.gif") no-repeat top left;
	height: 13px;
	margin-right:9px;
}
.lavaLampWithImage li a {
	text-decoration: none;
	color: #fff;
	outline: none;
	text-align: center;
	top: 0px;
	text-transform: uppercase;
	letter-spacing: 0;
	z-index: 10;
	display: block;
	float: left;
	height: 13px;
	line-height:13px;
	position: relative;
	overflow: hidden;
	width:38px;
/*	margin: auto 8px;
	_margin: auto 4px;*/
}
.lavaLampWithImage li a:hover,.lavaLampWithImage li a:active,.lavaLampWithImage li a:visited{
	border: none;
	color:#ffffff;
}


/*==================左边菜单===================*/
#nav {
	width:160px;
    line-height: 28px; 
	list-style-type: none;
	text-align:left;color:#000;
    /*定义整个ul菜单的行高和背景色*/
}

/*==================一级目录===================*/
#nav a {
	width:160px; 
	display: block;
	padding-left:10px;
	color:#000;
	/*Width(一定要)，否则下面的Li会变形*/
}
#nav li {
	/*background:#05a0cf; 一级目录的背景色*/
	/*border-bottom:1px solid #FFF;下面的一条白边*/
	float:left;color:#000;font-weight:600;
	/*float：left,本不应该设置，但由于在Firefox不能正常显示
	继承Nav的width,限制宽度，li自动向下延伸*/
}
#nav li a{border-bottom:1px solid #000;background:url("../images/ico.gif") no-repeat center left;padding-left:15px}
#nav li a:hover{
	color:red;
	background:url("../images/ico.gif") no-repeat center left; padding-left:15px/*#00A02D;	一级目录onMouseOver显示的背景色
	
}

#nav a:link  {
	color:#000; text-decoration:none;
	font-weight:600;
	/*background:url(style1/images/home/do-1.gif) no-repeat left center;*/
}
#nav a:visited  {
	color:#000;text-decoration:none;
	font-weight:bold;
}
#nav a:hover  {
	color:#FFF;text-decoration:none;font-weight:bold;
	background:#CC0000;
	
}

/*==================二级目录===================*/
#nav li ul {
	list-style:none;
	text-align:left;
	float:left;
}
#nav li ul li{	
	/*background-color: #82B9DD; 二级目录的背景色*/
	border-bottom:#000 1px dotted;
}

#nav li ul a{
         padding-left:5px;
         width:160px;
		 border:0px;
		 background:none;
	/* padding-left二级目录中文字向右移动，但Width必须重新设置=(总宽度-padding-left)*/
}

/*下面是二级目录的链接样式*/

#nav li ul a:link  {
	color:#000; text-decoration:none;
	font-weight:normal;
	line-height:28px;
	height:28px;
}
#nav li ul a:visited  {
	color:#000;text-decoration:none;
	font-weight:normal;
}
#nav li ul a:hover {
	color:red;
	text-decoration:none;
	font-weight:normal;
	padding-left:5px;
	background:none;
	/*background:#00A02D;
	 二级onmouseover的字体颜色、背景色*/
}
#nav li ul a span{ margin-right:5px}
#nav li:hover ul {
	left: auto;
}
#nav li.sfhover ul {
	left: auto;
}
#content {
	clear: left; 
}
#nav ul.collapsed {
	display: none;
}


