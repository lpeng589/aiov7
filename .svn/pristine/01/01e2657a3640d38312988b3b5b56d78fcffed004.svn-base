#ul_left .close-talk{display:none;right:-5px;top:-5px;width:13px;height:13px;background:url(/style/images/item/icons.png) no-repeat -180px -258px;position:absolute;z-index:999;}
#ul_left li:hover .close-talk{display:inline-block;}
.enter-list{position:relative;background:#eee;background-image:-webkit-linear-gradient(top,#f9f9f9,#f2f2f2);background-image:linear-gradient(top,#f9f9f9,#f2f2f2);border-top:1px #bbb solid;border-bottom:1px #bbb solid;}
.enter-list span{float:left;display:inline-block;cursor:pointer;}
.faceBtn{width:16px;height:16px;background-position:0 0;margin:2px 0 0 10px;}
.pictureBtn{width:16px;height:16px;background-position:-16px 0;margin:2px 0 0 10px;}
.afileBtn{width:16px;height:16px;background-position:-80px 0;margin:2px 0 0 10px;}
.faceBtn:hover,.pictureBtn:hover,.afileBtn:hover{}
.enter-list .s-history{float:right;padding:0 8px;cursor:pointer;color:#4086ce;}
.im-content{width:328px;height:60px;overflow:hidden;margin:5px 5px 0 5px;outline:0;border:0;}
.e-form{float:left;display:inline-block;}
.get-kk-btn{overflow:hidden;border-top:1px #bbb solid;}
.kk-icons{background-image:url(/style/v7/images/kk_icon16.jpg);background-repeat:no-repeat;display:inline-block;}
.hide-talk{position:absolute;right:28px;top:3px;font-size:32px;color:#fff;cursor:pointer;font-weight:bold;line-height:18px;}
.close-talk{z-index:99;width:13px;height:13px;right:5px;top:8px;cursor:pointer;background:url(/style/images/item/icons.png) no-repeat -153px -258px;display:inline-block;position:absolute;}

#showKK{position:fixed;left: 500px;top:200px;background-color:#fff;border:1px #4086CE solid;box-shadow:0px 1px 5px RGBA(0,0,0,0.5);}
.showKKlog{position:fixed;bottom:50px;right:65px;display: none; background:#4086CE;color:#fff;padding:0 15px;line-height:25px;cursor: pointer;}
.showKKlog-back{background-color:#da4f49}

/*通知 消息 下拉框*/
.message{cursor:default;z-index:1003;position:absolute;right:37px;bottom:5px;min-width:60px;}
.message i.sani{width:20px;height:11px;position:absolute;background:url(/style/images/client/784.png) no-repeat 0 0;right:28px;top:-10px;}
.msgpoup_middle{background:#fff;border:1px solid #468dc8;min-height:36px;min-weight:60px;}
.message a{color:#000000;font-size:12px;display:block;white-space:nowrap;padding:2px 8px;text-decoration:none;}
.message a:hover{background:#d6e5fc;}

.search_popup{width:350px;height:300px;border:6px #8FCFFF solid;position:absolute;z-index:99999;background:#fff;box-shadow:#666 0px 0px 10px;}
.move_dv{width:350px;height:62px;position:relative;background:url(/style/images/body2/ReadFileServlet.png) no-repeat 7px 5px;}
.search_popup .img_p{text-align:center;margin:10px 0 0 0;}
