body{ margin:0px; padding:0px; font-size:12px;}
body div {margin-right:auto; margin-left:auto;} 
ul,li,ol{ list-style:none; margin:0px; padding:0px;}
img,table{border:0px;}
#columns{width:100%;height:500px;overflow:auto;} 
#columns .column{margin:5px 0px 0px 10px; float:left;}
#columns .widget {width:100%;height:210px;overflow: hidden; margin: 10px 10px 10px 0px;background:#fff url(../images/desk/BlockTitle_bg.gif) left top repeat-x; border:1px solid #B4B4B4;}

.widget-placeholder { border: 2px dashed #999;}
#column1 .widget-placeholder { margin: 10px 10px 10px 10px; }
#column2 .widget-placeholder { margin: 10px 10px 10px 10px; }
#column3 .widget-placeholder { margin: 10px 10px 10px 10px; }

#columns .widget-head{width:97%; line-height:24px; float:left; padding:0px 8px 0px 8px; font-weight:bold; height:24px;}
#columns .widget-head div{ float:left;}
#columns .widget-head a{ float:right; margin:5px 0px 0px 10px; _font-size:1px;}
.ti_07{ background:url(../images/desk/ti_07.gif) 0px 0px;width:10px; height:10px;}
.ti_08{ background:url(../images/desk/ti_07.gif) -22px 0px;width:10px; height:10px;}
.ti_09{ background:url(../images/desk/ti_07.gif) -41px 0px;width:10px; height:10px;}

.desktop_right li table,.column .widget table,.Block_honor_list{width:95%; margin:10px; float:left;}
.desktop_right li table td a,.column .widget table td a{ text-decoration:none; color:#000; font-size:12px;}
.Block_news_list{width:100%; margin:0px;}
.Block_news_list td{margin:0px; background:none; border:0px; height:26px; line-height:26px;}
.Block_word_list td{width:100%; border:1px solid #fff; background:#F0F0F0; height:29px; line-height:29px; padding-left:5px;
					 white-space:nowrap;overflow: hidden;text-overflow: ellipsis;}
.Block_honor_list li{width:80px; height:auto;float:left;  text-align:center; border:0px; background:none; margin:-5px 8px 0px 6px; _margin:0px 5px 0px 5px;}
.Block_honor_list li div{ margin:3px 0px 5px;}
.Block_honor_list li div a{ text-decoration:none; color:#000; font-size:12px; }
.Block_honor_list li div img{width:75px; height:93px; border:4px solid #E8E8E8;}
.Block_honor_list li span a{ margin:0px 2px 0px 2px;}
.Block_honor_info{ float:left; width:100%;padding-left:15px; height:156px; position:relative;}
.Block_honor_info div{width:100%;}
.Block_honor_info div div{width:auto; margin:5px 15px 0px 0px; height:auto; float:left; position:relative;}
.Block_honor_info div div div{z-index:9999;width:16px; height:16px; text-align:center; line-height:16px; color:#ff0000; position:absolute; top:-3px; right:-15px;}
#flow{ width:88%; float:left; position:absolute; bottom:0px; left:15px; color:#9D5900;}

#YSlide{position:relative;z-index:1;margin:0 auto;width:auto;height:180px;padding:4px;overflow:hidden;}
#YSlide .YSample{margin:0 auto;overflow:hidden;padding:0;}
#YSlide .YSample,#YSlide .YSample img{width:100%;height:180px;border:0px;}
#YSlide .YSample strong{position:absolute;z-index:5;bottom:2px;left:4px;font-weight:normal;width:auto;height:25px;line-height:25px;text-indent:10px;font-size:14px;color:#000;background-color:#fff;opacity:.7;-moz-opacity:.7;filter:alpha(opacity=70);overflow:hidden;padding:0;}
#YSlide #jSIndex{position:absolute;z-index:6;top:120px;right:8px;width:100%;text-align:right;height:25px;line-height:25px;overflow:hidden;padding:0;}
#jSIndex a:link,#jSIndex a:visited,#jSIndex a:hover{position:relative;z-index:6;padding:2px 4px;border:1px solid #FFF;color:#FFF;margin-right:5px;font-weight:bold;text-decoration:none;font-size:10px;}
#jSIndex a:hover,#jSIndex a.current:link,#jSIndex a.current:visited,#jSIndex a.current:hover{background-color:#35AADF;}