ays><Display localStr="zh_CN" display="剩余保修（月）"/></Displays><groupNames/></field></fieldInfo><SQL text="select vbCrLf    CRMSaleContract.BillDate as BillDate,vbCrLf    CRMSaleContract.ContractNo as ContractNo,vbCrLf    CRMSaleContract.ContractType as ContractType,vbCrLf    CRMClientInfo.ClientName as ClientName,vbCrLf    CRMClientInfo.Phone as Phone,vbCrLf    CRMClientInfo.Fax as Fax,vbCrLf    tblEmployee.EmpFullName as EmpFullName,vbCrLf    tblDepartment.DeptFullName as DeptFullName,vbCrLf    CRMSaleContract.SignUpDate as SignUpDate,vbCrLf    CRMSaleContract.DeliveryDate as DeliveryDate,vbCrLf    CRMSaleContract.RepairTerm as RepairTerm,vbCrLf    CRMSaleContract.ServiceStartDate as ServiceStartDate,vbCrLf    CRMSaleContract.ServiceEndDate as ServiceEndDate,vbCrLf    tblGoods.GoodsNumber as GoodsNumber,vbCrLf    tblGoods.GoodsFullName as GoodsFullName,vbCrLf    tblGoods.GoodsSpec as GoodsSpec,vbCrLf    CRMSaleContractDet.Qty as Qty,vbCrLf    CRMSaleContractDet.MachineNo as MachineNo,vbCrLf    CRMSaleContractDet.MachineType as MachineType,vbCrLf    CRMSaleContractDet.Redo as Redo,vbCrLf    CRMSaleContractDet.RedoRemark as RedoRemark,vbCrLf    Repair.RepairNum as RepairNum,vbCrLf    CRMSaleContract.id as idvbCrLffrom CRMSaleContractvbCrLfleft join tblEmployee on CRMSaleContract.EmployeeID=tblEmployee.idvbCrLfleft join tblDepartment on CRMSaleContract.DepartmentCode=tblDepartment.classCodevbCrLfleft join CRMClientInfo on CRMClientInfo.id=CRMSaleContract.ClientIdvbCrLfleft join CRMSaleContractDet on CRMSaleContractDet.f_ref=CRMSaleContract.idvbCrLfleft join tblGoods on tblGoods.classCode=CRMSaleContractDet.GoodsCodevbCrLfleft join (select m.GoodsCode,m.MachineNo,n.RefBillID,count(n.RefBillID) as RepairNum from tblMaintainNoteDet m join tblMaintainNote n on m.f_ref=n.idvbCrLfgroup by m.GoodsCode,m.MachineNo,n.RefBillID) Repair on Repair.RefBillID=CRMSaleContract.id and Repair.GoodsCode=CRMSaleContractDet.GoodsCode and Repair.MachineNo=CRMSaleContractDet.MachineNovbCrLfvb