.jericho_tabs .img{ float:right; display:block;}

.tab_selected{width:100px; height:25px; background:url(/style/images/body2/tab_bg_x.png) no-repeat;line-height:25px;margin-left:-1px;margin-right:1px;}
.tab_unselect{float:left;background:url(/style/images/body2/tab_bg.png) no-repeat;height:25px; line-height:25px;cursor: pointer;margin-right:1px;}
.tab_selected span{color:#fff;}
.tab_selected .tab_close{background:url(/style/images/body/i_006_w.png) no-repeat 0 0;}
.tab_unselect span{color:#333;}
.tab_unselect .tab_close{background:url(/style/images/body/i_006.png) no-repeat 0 0;}
.jericho_tab{width:100%;margin-left:1px;overflow:hidden;}
.jericho_tab .tab_pages{width:100%;float: left;clear: both;margin:2px 0 0 0;}
.jericho_tab .tab_pages .tabs{height:25px;overflow :hidden;display:block;position:relative;float:left;}
.jericho_tab .tab_pages div{float: left;}
.jericho_tab .tab_pages ul, .jericho_tab .tab_pages li{padding: 0;list-style-type: none;}
.jericho_tab .tab_pages ul{position:relative;display:block;}
.jericho_tab .tab_pages ul li {float:left;display:inline-block;margin:0;padding:0;position:relative;}
.jericho_tab .tab_pages .tab_unselect div{height: 27px;line-height: 27px;}
.jericho_tab .tab_pages .tab_selected div{height: 27px;line-height: 30px;}
.jericho_tab .tab_pages .jericho_tabs div.tab_left{width: 145px;}
.jericho_tab .tab_pages .jericho_tabs div.tab_right{width: 5px;}
.jericho_tab .tab_pages .tab_unselect{cursor: pointer;}
.jericho_tab .tab_pages .tab_unselect .tab_left{margin-top: 3px;background: url(../images/jerichotab/jerichotab.gif) no-repeat left 0px;}
.jericho_tab .tab_pages .tab_unselect .tab_right{margin-top: 3px;background: url(../images/jerichotab/jerichotab.gif) no-repeat right -24px;}
.jericho_tab .tab_pages .tab_unselect_h .tab_left{margin-top: 3px;background: url(../images/jerichotab/jerichotab.gif) no-repeat left -104px;}
.jericho_tab .tab_pages .tab_unselect_h .tab_right{margin-top: 3px;background: url(../images/jerichotab/jerichotab.gif) no-repeat right -128px;}
.jericho_tab .tab_pages .tab_selected div.tab_left{background: url(../images/jerichotab/jerichotab.gif) no-repeat left -48px;}
.jericho_tab .tab_pages .tab_selected div.tab_right{background: url(../images/jerichotab/jerichotab.gif) no-repeat right -76px;}
.jericho_tab .tab_pages .jericho_tabs .tab_left .tab_icon{width: 25px;text-align: right;background-repeat: no-repeat;}
.jericho_tab .tab_pages .tab_selected .tab_left .tab_icon{background-position: right 7px;	}
.jericho_tab .tab_pages .tab_unselect .tab_left .tab_icon{background-position: right center;	}
.jericho_tab .tab_pages .jericho_tabs .tab_left .tab_text{font-family: Calibri;font-size: 12px;text-indent: 2px;width: 100px;}
.jericho_tab .tab_pages .jericho_sliders{width: 10px;height: 23px;margin:0;margin-top: 7px;cursor: pointer;float:left;margin-left:5px;}
.jericho_tab .tab_pages .jericho_sliderleft_enable{background: url(/style/images/body/i_left.gif) no-repeat;}
.jericho_tab .tab_pages .jericho_sliderleft_disable{background: url(/style/images/body/i_left.gif) no-repeat;}
.jericho_tab .tab_pages .jericho_sliderright_enable{background: url(/style/images/body/i_right.gif) no-repeat;}
.jericho_tab .tab_pages .jericho_sliderright_disable{background: url(/style/images/body/i_right.gif) no-repeat;}
.jericho_tab .tab_content{border: solid 1px red;border-top: solid 6px #CEDDFA;float: left;clear: both;width: 100%;background-color:#fff;}
.jericho_tab .tab_content div.content{font-family: Calibri;font-size: 12px;padding: 10px;position:relative;overflow:hidden;display:block;}
.jericho_tab .tab_content div.content .righttag{position:relative;top:0px;width:80px;height:20px;line-height:20px;background:#EEF3F7 url(../images/jerichotab/smallloader.gif) no-repeat 8px center;border:1px solid #A5B6C8;padding-left:30px;font-family:10px;}

/*收藏更多的时候 滚动条*/
#scroll{float:left;}
#scroLeft{float:left;height:100%;overflow:hidden;width:166px;}
#scroRight{float:right;height:100%;width:5px;border-radius:5px;overflow:hidden;margin-left:3px;cursor:pointer;}
#scroLine{position:absolute;top:10px;right:5px;width:5px;background:#fff;opacity:0.7;border-radius:5px;cursor:pointer;}

/*通知 消息 下拉框*/
.message{cursor:default;z-index:1000; position:absolute; right:84px; top:45px;
}
.message i.sani{width:20px;height:11px;position:absolute;background:url(/style/images/client/784.png) no-repeat 0 0;right:28px;top:-10px;}
.msgpoup_middle{background:#fff;border-radius:4px;border:1px solid #666;box-shadow:0 0 10px rgba(0,0,0,.5);}
.message a{color:#000000;font-size:12px;display:block;white-space:nowrap;width:55px;padding:2px 8px;text-decoration:none;}
.message a:hover{background:#d6e5fc;}

.advice{z-index:1000;position:absolute;display:none;border-radius:4px;border:1px solid #666;box-shadow:0 0 10px rgba(0,0,0,.5);right:0;top:45px;width:323px;max-height:230px;float:left;min-height:100px;}
.advice i.sani{width:20px;height:11px;position:absolute;background:url(/style/images/client/784.png) no-repeat 0 0;left:140px;top:-11px;}
.interior{float:left;border-radius:4px;width:100%;height:100%; background:#fff;}
.interior_top{float:left;width:100%; font-weight:bold; height:30px; line-height:30px;}
.interior_top img{vertical-align:middle;}
.interior_top div{ float:left;margin-left:5px;}
.interior_top span{ float:right;margin-right:10px;}
.interior_c{ float:left; width:312px; height:100%; overflow:auto;max-height:195px;min-height:100px;padding:5px;padding-top:0px;}
.interior_c a{text-decoration:none;color:#333}
.interior_c img{cursor:pointer;}
.interior_c table{ width:100%; background:#f7f7f7;}
.interior_c table td{ height:29px; line-height:20px; padding:0px 5px 0px 5px;font-size:12px;color:#333;}
.interior_c td a{float:left;width:260px;white-space:nowrap;text-overflow:ellipsis;overflow:hidden;display:block;}
.interior_c table tbody{ overflow:auto; overflow-y:auto}
.noAdvice{background:#f7f7f7;height:100px;line-height:100px;}
/*手机客户端-二维码*/
.search_popup{width:350px;height:300px;border:6px #8FCFFF solid;position:absolute;z-index:99999;background:#fff;box-shadow:#666 0px 0px 10px;}
.move_dv{width:350px;height:62px;position:relative;background:url(/style/images/body2/ReadFileServlet.png) no-repeat 7px 5px;}
.search_popup .img_p{text-align:center;margin:10px 0 0 0;}
/*********************分割线  滚动条 Start  *******************/
::-webkit-scrollbar {
	width:8px;
	height:8px;
}

::-webkit-scrollbar-button {}
::-webkit-scrollbar-button:start:decrement,::-webkit-scrollbar-button:end:increment {
	width:0;
	height:0
}
::-webkit-scrollbar-track-piece {
	background-clip:padding-box;
	background-color:whitesmoke;
	border:none;
}

::-webkit-scrollbar-thumb {
	width:6px;
	height:6px;
	overflow:visible;
	border-radius:4px;
	border:solid 1px #A6A6A6;
	background-color:#92caeb;
}

/*********************分割线  滚动条 End  *******************/