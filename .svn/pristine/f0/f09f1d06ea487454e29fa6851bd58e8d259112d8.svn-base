yOrder on tblBuyOrder.id=tblBuyInStockDet.BuyOrderIDvbCrLf left join tblemployee emp2 on emp2.id=tblBuyInStock.lastUpdateByvbCrLfwhere 1=1 vbCrLf      and (tblBuyInStock.BillDate &gt;= ?)vbCrLf      and (tblBuyInStock.BillDate &lt;= ?)vbCrLf      and (tblBuyInStock.BillNo like '%'+?+'%')vbCrLf      and (tblCompany.ComFullName like '%'+?+'%')vbCrLf      and (tblBuyInStock.InVoiceType like '%'+?+'%')vbCrLf      and (tblEmployee1.EmpFullName like '%'+?+'%')vbCrLf      and (tblDepartment.DeptFullName like '%'+?+'%')vbCrLf      and (tblBuyOrder.BillNo like '%'+?+'%')vbCrLf      and (tblEmployee2.EmpFullName like '%'+?+'%')vbCrLf      and (tblBuyInStock.Remark like '%'+?+'%')vbCrLfvbCrLforder by vbCrLf   tblBuyInStock.BillDate desc,vbCrLf   tblBuyInStock.BillNo descvbCrLfvbCrLf"/><charts/></ReportDataSource>
