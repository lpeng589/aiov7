package com.koron.oa.bean;
/**
 * <p> Title:阅读痕迹.实体类 </p>
 * 
 * <p>Description:用于保存用户阅读内容记录</p>
 * 
 * <p> Copyright: Copyright (c) 2010 </p>
 * 
 * <p> Company: 科荣软件 </p>
 * 
 * @author 汤晶
 * @version 4.0
 */
public class OAReadingRecord {
	 private String id;              //主键ID
	 private String readingInfoTable;//阅读信息所属对象表
	 private String readingInfoId;   //阅读信息ID
	 private String readingTime;     //访问时间
	 private String reader;          //阅读用户
	 private String visitMachine;    //访问机器名称
	 private String ip;              //用户访问IP地址
	 private int visitCount;         //访问次数
	 
	public OAReadingRecord() {
		super();
	}
	public OAReadingRecord(String id, String readingInfoTable, String readingInfoId, String readingTime, String reader, String visitMachine, String ip, int visitCount) {
		super();
		this.id = id;
		this.readingInfoTable = readingInfoTable;
		this.readingInfoId = readingInfoId;
		this.readingTime = readingTime;
		this.reader = reader;
		this.visitMachine = visitMachine;
		this.ip = ip;
		this.visitCount = visitCount;
	}
	/*
	 * 主键ID
	 */
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	/*
	 * 用户访问IP地址
	 */
	public String getIp() {
		return ip;
	}
	public void setIp(String ip) {
		this.ip = ip;
	}
	/*
	 * 阅读用户
	 */
	public String getReader() {
		return reader;
	}
	public void setReader(String reader) {
		this.reader = reader;
	}
	/*
	 * 阅读信息ID
	 */
	public String getReadingInfoId() {
		return readingInfoId;
	}
	public void setReadingInfoId(String readingInfoId) {
		this.readingInfoId = readingInfoId;
	}
	/*
	 * 阅读信息所属对象表
	 */
	public String getReadingInfoTable() {
		return readingInfoTable;
	}
	public void setReadingInfoTable(String readingInfoTable) {
		this.readingInfoTable = readingInfoTable;
	}
	/*
	 * 阅读时间
	 */
	public String getReadingTime() {
		return readingTime;
	}
	public void setReadingTime(String readingTime) {
		this.readingTime = readingTime;
	}
	/*
	 * 访问次数
	 */
	public int getVisitCount() {
		return visitCount;
	}
	public void setVisitCount(int visitCount) {
		this.visitCount = visitCount;
	}
	/*
	 * 访问机器名称
	 */
	public String getVisitMachine() {
		return visitMachine;
	}
	public void setVisitMachine(String visitMachine) {
		this.visitMachine = visitMachine;
	}

}
