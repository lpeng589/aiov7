public class MobileFlowAction {

	private String id ;
	private String name ;
	private String to ;
	private boolean fixedApp ;
	private boolean cancel ;
	private int ideaRequired ;
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getTo() {
		return to;
	}
	public void setTo(String to) {
		this.to = to;
	}
	public boolean isFixedApp() {
		return fixedApp;
	}
	public void setFixedApp(boolean fixedApp) {
		this.fixedApp = fixedApp;
	}
	public boolean isCancel() {
		return cancel;
	}
	public void setCancel(boolean cancel) {
		this.cancel = cancel;
	}
	public int isIdeaRequired() {
		return ideaRequired;
	}
	public void setIdeaRequired(int ideaRequired) {
		this.ideaRequired = ideaRequired;
	}
}
