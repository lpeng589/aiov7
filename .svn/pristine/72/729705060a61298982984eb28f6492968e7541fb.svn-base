ame="" fieldIdentity="Amount" analysis="0"><Displays><Display localStr="zh_CN" display="应付余额"/><Display localStr="en" display="Balance of A/P"/><Display localStr="zh_TW" display="應付餘額"/></Displays><groupNames/></field><field fieldname="(case when '@Sess:Local'='zh_CN' then '应付期初明细' when '@Sess:Local'='zh_TW' then '應付期初明細' when '@Sess:Local'='en' then 'Pay Initial Detail' end)" AsfieldName="PayList" groupByFlag="0" displayFlag="1" condition="" conditionJoin="" orderbyFlag="" display="应付期初明细" fieldType="" inputType="" popUpName="" width="90" classCode="1" linkAdd="ReportDataAction.do?reportNumber=ReportCompanybeginning&amp;CompanyCode=@ValueofDB:classCode&amp;hide_CompanyCode=@ValueofDB:hidecode&amp;tblCompany.ComFullName=@ValueofDB:ComFullName&amp;BeginningType=1" order="14" isStat="0" defaultValue="" fieldSysType="" fixColName="1" isNull="0" planarField="0" subSQL="" repeatNotShow="0" zeroDisplay="0" seriesCol="" seriesNums="" crossField="" groupName="" fieldIdentity="" analysis="0"><Displays><Display localStr="zh_CN" display="应付期初明细"/><Display localStr="en" display="Pay Initial Detail"/><Display localStr="zh_TW" display="應付期初明細"/></Displays><groupNames/></field></fieldInfo><SQL text="select vbCrLf    tblCompany.ComNumber as ComNumber,vbCrLf    tblCompany.classCode as CompanyCode,vbCrLf    tblCompany.ComFullName as ComFullName,vbCrLf    tblCompanyTotal.PayBegin as PayBegin,vbCrLf    tblCompanyTotal.PayTotalDebit as PayTotalDebit,vbCrLf    tblCompanyTotal.PayTotalLend as PayTotalLend,vbCrLf    tblCompanyTotal.PayTotalRemain as PayTotalRemain,vbCrLf    tblCompany.ClientFlag as ClientFlag,vbCrLf    tblCompany.SCompanyID as SCompanyID,vbCrLf    tblCompany.isCatalog as isCatalog,vbCrLf    tblCompany.classCode+'#;#'+convert(varchar(2),tblCompany.isCatalog)+'#;##;#' as hidecode,vbCrLf    tblCompany.classCode as classCode,vbCrLf    tblCompanyTotal.PayTotalRemain as PayTotalRemain1,vbCrLf    (case when '@Sess:Local'='zh_CN' then '应付期初明细' when '@Sess:Local'='zh_TW' then '應付期初明細' when '@Sess:Local'='en' then 'Pay Initial Detail' end) as PayListvbCrLffrom tblCompanyvbCrLfouter apply (	select 			vbCrLf					isnull(sum(BeginAmount),0) as PayBegin,vbCrLf					isnull(sum(TotalDebit),0) as PayTotalDebit,vbCrLf					isnull(sum(TotalLend),0) as PayTotalLend,vbCrLf					isnull(sum(TotalRemain),0) as PayTotalRemain,vbCrLf					isnull(sum(CurBeginAmount),0) as CurPayBegin,vbCrLf					isnull(sum(CurTotalDebit),0) as CurPayTotalDebit,vbCrLf					isnull(sum(CurTotalLend),0) as CurPayTotalLend,vbCrLf					isnull(sum(CurTotalRemain),0) as CurPayTotalRemainvbCrLf					from tblCompanybeginningDet x ,tblCompanybeginningvbCrLfWHERE tblCompanybeginning.BeginningType ='1' ANDvbCrLf				 x.SCompanyID=tblCompany.SCompanyIDvbCrLf					and x.CompanyCode like tblCompany.classCode+'%' AND tblCompanybeginning.id = x.f_ref) as tblCompanyTotalvbCrLfwhere 1=1 vbCrLf      and (tblCompany.classCode =?)vbCrLf      and (tblCompany.ClientFlag = 1 or tblCompany.ClientFlag=3)vbCrLf      and (tblCompany.SCompanyID like '@Sess:SCompanyID'+'%')vbCrLf      and (tblCompanyTotal.PayTotalRemain &lt;&gt; ?)vbCrLfvbCrLforder by vbCrLf   tblCompany.classCode ,vbCrLf   tblCompany.ComFullName vbCrLfvbCrLf"/><charts/></ReportDataSource>
