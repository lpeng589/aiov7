/* CSS Document */
body{
	width:100%;
	margin:0 auto;
	padding-left:10px;
	padding-right:10px;
	font-size:12px;
	overflow-x:hidden;
	background:url(../images/login/bg.jpg) no-repeat right top;
}
a:link {
	text-decoration: none;
	color:#1A66B3;
}
a:visited {
	text-decoration: none;
	color:#1A66B3;
}
a:hover {
	text-decoration:underline;
	color:#1A66B3;
}
a:active {
	text-decoration: none;
	color:#1A66B3;
}
img {
	border:0px;
}
li {
	list-style:none;
}
.login_all {
	width:100%;
	padding-top:10px;
}
.login_min {
	width:100%;
}
.logo {
	float:left;
	padding-left:10px;
	width:98%;
	height:150px;
	overflow:hidden;
	text-align:left;
}
.login_minleft {
	float:left;
	width:60%;
	border:1px #FF0000 solid;
	height:340px;
}
.login_in {
	float:left;
	width:300px;
	height:340px;
	border:1px #0000ff solid;
}
.login_minright {
	float:right;
	height:340px;
	width:100px;
	border:1px #cccccc solid;
}
.right_bg {
	width:107px;
	height:226px;
	background:url(../images/login/right_bg.gif) no-repeat;

}
.right_bg2 {
	width:106px;

}

.login_minbg {
	background:url(../images/login/login_minbg.gif) repeat-y;
	width:312px;
}
.koron_info {
	vertical-align:baseline;
	padding:0px;
	margin:0px;
	font-size:12px;
	height:35px;
	line-height:35px;
	text-indent:10px;
	white-space:nowrap;
	overflow: visible;
	text-overflow: ellipsis;
	color:#666;
}
.koron_info span {
	vertical-align:middle;
}
.koron_info li {
	float:left;
	text-align:left;
}
.login_into {
	width:290px;
	padding:0px;
	margin:0px;
}
.login_into li {
	float:left;
	width:290px;
	height:35px;
	color:#ffffff;
	text-align:center;
	font-weight:600;
	font-size:14px;
	list-style:none;
}
.login_into div{
	width:100px;
	height:35px;
	line-height:25px;
	float:left;
	text-align:right;
}
.login_into span {
	width:150px;
	float:left;
	text-align:left;
}
.intext {
	width:150px;
	color:#666666;
	font-weight:600;
	font-size:14px;
	height:24px;
	border: 1px solid #0F71C3;
	background-color: #e7f2fe;
	padding: 2px 3px 3px 3px;
}
.intext_hover {
background-color: #FFFCE4;
border: 1px solid #EBA428;
}
.login_into select {
	width:150px;
}
.login_user {
	padding:0px;
	margin:0px;
}
.login_user li {
	width:107px;
	float:left;
	color:#FFFFFF;
	font-size:13px;
	font-weight:600;
	margin-left:98px;
	height:40px;
	margin-top:10px;
	text-align:right;
}
#sunCom { padding-top:5px; font-size:12px; color:#D9F4F4; font-weight:normal; text-align:right;}
.login_user button {
	width:107px;
	height:40px;
	border:0px;
	background:url(../images/login/login_buttonbg.gif) no-repeat;
	background-color:transparent;
	font-size:16px;
	color:#ffffff;
	font-weight:bolder;
	text-align:center;
	cursor:pointer;
}
.left_bp {
	width:287px;
	height:206px;
	margin:0px;
	padding:0px;

}
.bigbg_tr{width:377px; height:226px; overflow:hidden; background:#2B9BD5}
.bigbg {
	
	background: #2B9BD5 url(../images/login/left_bg.gif) no-repeat center right;

}
.login_l {
	width:17px;
	height:226px;
}

.login_top {
	background:url(../images/login/logo_top.gif) no-repeat center bottom;
	width:312px;
	height:60px;
}
.login_top span{ text-indent:65px;width:136px; font-size:14px; float:left; font-weight:bold;color:#ffffff;}
.line { border-bottom:1px solid #D5D5D5; width:1000px; overflow:hidden; height:20px;}
.foot { font-size:12px; color:#666666; height:30px; line-height:30px;}
.k_info { color:1A66B3; font-size:12px; width:313px; overflow:hidden; float:left; margin:70px 1px 10px 0px;} 
.k_info a:link {
	text-decoration: none;
	color:#1A66B3;
}
.k_info a:linka:visited {
	text-decoration: none;
	color:#1A66B3;
}
.k_info a:hover {
	text-decoration:underline;
	color:#1A66B3;
}
.k_info a:active {
	text-decoration: none;
	color:#1A66B3;
}