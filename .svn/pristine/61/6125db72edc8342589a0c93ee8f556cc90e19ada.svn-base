<?xml version="1.0" encoding="gb2312"?>
<popupSelect>
	<!--
		displayFields 中的字段全都是会显示在弹出页面的字段
		parentDisplay为true表示该字段需在主表界面显示，但不一定会保存在主表，由saveFields决定，
		如果在主表显示，并配有display则主表显示字段对应的标签名取display的字段名，
		searchType no:不作为查询条件，equal：等于，match：模糊查询,scope：区间查询
		saveFields 中的字段全都是必须在主表保存的字段
		其中必须有且仅有一个relationKey="true"的字段。该字段表示的是关联字段，关联查询通过这个字段建立
		关联关系，该关联字段在主表是否显示由hidden属性决定，如果为true表示隐藏，界面不显示，通过隐藏变
		量传递值。
		其它非关联字段，在主表必须存在，且可录入。
	-->

	<!--通用型弹出窗选择配置文件-->
	<!---以下是往来单位、仓库、职员、部门、商品、辅助单位等基本信息的选择配置-->
	
	<!-- OA工作流类型查询 -->
	<select name="OAMyWorkFlowType">
		<table>OAWorkFlowTemplate,tblWorkFlowType</table>
		<relation>
			on OAWorkFlowTemplate.templateClass=tblWorkFlowType.classCode
		</relation>
		<condition>
			templateStatus=1 and OAWorkFlowTemplate.statusId=0 
		</condition>
		<displayFields>
			<field name="OAWorkFlowTemplate.templateName" width="200" searchType="match" parentDisplay="false" />
			<field name="tblWorkFlowType.ModuleName" width="150" searchType="match" parentDisplay="false" />
		</displayFields>
		<saveFields>
			<field name="OAWorkFlowTemplate.templateName" parentName="tblOtherProgramePathsDet.userName" relationKey="true" hidden="true" />
		</saveFields>
	</select>
	
	<select name="OAEmployee">
	     <table>tblEmployee,tblDepartment</table>
	     <forwardModel></forwardModel>
	     <relation></relation>
	     <saveParentFlag></saveParentFlag>
	     <defineSQL>select 
	      tblEmployee.id f1,
	      tblEmployee.DepartmentCode f2,
	      tblDepartment.DeptFullName f3,
	      tblEmployee.EmpNumber f4,
	      tblEmployee.EmpFullName f5,
	      tblEmployee.EmpFullNamePYM f6,
	      tblEmployee.TitleID f7,
	      tblDepartment.DeptFullName f8,
      	tblDepartment.DeptFullNamePYM f9
	  		from tblEmployee left join tblDepartment on tblEmployee.DepartmentCode=tblDepartment.classCode
	     </defineSQL>
	     <classCode></classCode>
	     <remark></remark>
	     <condition>((tblEmployee.SCompanyID='@Sess:SCompanyID' and (tblEmployee.statusId!='-1' or tblEmployee.id='1') and tblEmployee.workFlowNodeName='finish') and tblEmployee.workFlowNodeName='finish' and tblDepartment.workFlowNodeName='finish')</condition>
	     <changeCond></changeCond>
	     <orderBy>tblEmployee.EmpNumber,tblEmployee.EmpFullName</orderBy>
	     <displayFields>
	          <field name="tblEmployee.EmpNumber" width="100" searchType="match" keySearch="true" parentDisplay="false" orderBy="升" />
	          <field name="tblEmployee.EmpFullName" width="65" searchType="match" keySearch="true" parentDisplay="true" display="@TABLENAME.EmployeeID" orderBy="升" />
	          <field name="tblEmployee.EmpFullNamePYM" width="80" searchType="match" keySearch="true" parentDisplay="false" hiddenInput="true" />
	          <field name="tblEmployee.TitleID" width="65" searchType="equal" keySearch="true" parentDisplay="false" />
	          <field name="tblDepartment.DeptFullName" width="75" searchType="match" keySearch="true" parentDisplay="false" display="@TABLENAME.DepartmentCode" />
          	<field name="tblDepartment.DeptFullNamePYM" width="100" searchType="match" keySearch="true" parentDisplay="false" hiddenInput="true" />
	     </displayFields>
	     <saveFields>
	          <field name="tblEmployee.id" parentName="@TABLENAME.EmployeeID" relationKey="true" hidden="true" />
	          <field name="tblEmployee.DepartmentCode" parentName="@TABLENAME.DepartmentCode" relationKey="false" hidden="true" />
	          <field name="tblDepartment.DeptFullName" parentName="tblDepartment_DeptFullName" relationKey="false" hidden="true" />
	     </saveFields>
	</select>
	
	<select name="OADepartment">
     <table>tblDepartment</table>
     <forwardModel></forwardModel>
     <saveParentFlag>true</saveParentFlag>
     <relation></relation>
     <defineSQL>select 
      tblDepartment.classCode f1,
      tblDepartment.DeptCode f2,
      tblDepartment.DeptFullName f3,
      tblDepartment.DeptFullNamePYM f4
			from tblDepartment
     </defineSQL>
     <classCode>tblDepartment.classCode</classCode>
     <condition></condition>
     <changeCond>(tblDepartment.statusid!='-1') and tblDepartment.workFlowNodeName='finish'</changeCond>
     <orderBy>tblDepartment.DeptCode,tblDepartment.DeptFullName</orderBy>
     <displayFields>
          <field name="tblDepartment.DeptCode" width="70" searchType="match" keySearch="true" parentDisplay="false" />
          <field name="tblDepartment.DeptFullName" width="70" searchType="match" keySearch="true" parentDisplay="true" display="@TABLENAME.DepartmentCode" />
          <field name="tblDepartment.DeptFullNamePYM" width="100" searchType="match" keySearch="true" parentDisplay="false" hiddenInput="true" />
     </displayFields>
     <saveFields>
          <field name="tblDepartment.classCode" parentName="@TABLENAME.DepartmentCode" relationKey="true" hidden="true" />
     </saveFields>
 </select>
	
