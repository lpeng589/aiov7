eld="" groupName="" fieldIdentity="Amount" analysis="0"><Displays><Display localStr="zh_CN" display="应收余额"/><Display localStr="en" display="Remaining not equal"/><Display localStr="zh_TW" display="應收餘額"/></Displays><groupNames/></field><field fieldname="(case when '@Sess:Local'='zh_CN' then '应收期初明细' when '@Sess:Local'='zh_TW' then '應收期初明細' when '@Sess:Local'='en' then 'Receive Initial Detail' end)" AsfieldName="ReceiveList" groupByFlag="0" displayFlag="1" condition="" conditionJoin="  " orderbyFlag="" display="应收期初明细" fieldType="2" inputType="0" popUpName="" width="90" classCode="1" linkAdd="ReportDataAction.do?reportNumber=ReportCompanybeginning&amp;CompanyCode=@ValueofDB:classCode&amp;hide_CompanyCode=@ValueofDB:hidecode&amp;tblCompany.ComFullName=@ValueofDB:ComFullName&amp;BeginningType=2" order="20" isStat="0" defaultValue="" fieldSysType="" fixColName="1" isNull="0" planarField="0" subSQL="" repeatNotShow="0" zeroDisplay="0" seriesCol="" seriesNums="" crossField="" groupName="" fieldIdentity="" analysis="0"><Displays><Display localStr="zh_CN" display="应收期初明细"/><Display localStr="en" display="AR Detail"/><Display localStr="zh_TW" display="應收期初明細"/></Displays><groupNames/></field></fieldInfo><SQL text="select vbCrLf    tblCompany.ComNumber as ComNumber,vbCrLf    tblCompany.ComFullName as ComFullName,vbCrLf    tblCompanyTotal.ReceiveBegin as ReceiveBegin,vbCrLf    tblCompanyTotal.ReceiveTotalDebit as ReceiveTotalDebit,vbCrLf    tblCompanyTotal.ReceiveTotalLend as ReceiveTotalLend,vbCrLf    tblCompanyTotal.ReceiveTotalRemain as ReceiveTotalRemain,vbCrLf    tblCompany.ClientFlag as ClientFlag,vbCrLf    tblCompany.SCompanyID as SCompanyID,vbCrLf    tblCompany.isCatalog as isCatalog,vbCrLf    tblCompany.classCode+'#;#'+convert(varchar(2),tblCompany.isCatalog)+'#;##;#' as hidecode,vbCrLf    tblCompany.classCode as classCode,vbCrLf    tblCompanyTotal.ReceiveTotalRemain as ReceiveTotalRemain1,vbCrLf    (case when '@Sess:Local'='zh_CN' then '应收期初明细' when '@Sess:Local'='zh_TW' then '應收期初明細' when '@Sess:Local'='en' then 'Receive Initial Detail' end) as ReceiveListvbCrLffrom tblCompanyvbCrLfouter apply (select 			vbCrLf					isnull(sum(BeginAmount),0) as ReceiveBegin,vbCrLf					isnull(sum(TotalDebit),0) as ReceiveTotalDebit,vbCrLf					isnull(sum(TotalLend),0) as ReceiveTotalLend,vbCrLf					isnull(sum(TotalRemain),0) as ReceiveTotalRemain,vbCrLf					isnull(sum(CurBeginAmount),0) as CurReceiveBegin,vbCrLf					isnull(sum(CurTotalDebit),0) as CurReceiveTotalDebit,vbCrLf					isnull(sum(CurTotalLend),0) as CurReceiveTotalLend,vbCrLf					isnull(sum(CurTotalRemain),0) as CurReceiveTotalRemainvbCrLf					from tblCompanybeginningDet x ,tblCompanybeginningvbCrLfWHERE tblCompanybeginning.BeginningType ='1' ANDvbCrLf				 x.SCompanyID=tblCompany.SCompanyIDvbCrLf					and x.CompanyCode like tblCompany.classCode+'%' AND tblCompanybeginning.id = x.f_ref) as tblCompanyTotalvbCrLfwhere 1=1 vbCrLf      and (tblCompany.classCode =?)vbCrLf      and (tblCompany.ClientFlag =2 or tblCompany.ClientFlag=3)vbCrLf      and (tblCompany.SCompanyID like '@Sess:SCompanyID'+'%')vbCrLf      and (tblCompanyTotal.ReceiveTotalRemain &lt;&gt; ?)vbCrLfvbCrLforder by vbCrLf   tblCompany.classCode ,vbCrLf   tblCompany.ComFullName vbCrLfvbCrLf"/><charts/></ReportDataSource>
