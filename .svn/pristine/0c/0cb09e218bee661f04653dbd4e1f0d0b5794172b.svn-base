ullName as DeptFullName,vbCrLf    tblEmployee2.EmpFullName as sysName,vbCrLf    tblOtherOut.id as id,vbCrLf    tblCompany.[tblCompany_2] as ComFullName,vbCrLf    tblOtherOut.OtherInNo as OtherInNo,vbCrLf    tblOtherOut.tblOtherInID as tblOtherInIDvbCrLffrom tblOtherOut  vbCrLf left join tblOtherOutDet on (tblOtherOut.id=tblOtherOutDet.f_ref and tblOtherOutDet.id=(select top 1 a.id from tblOtherOutDet a where a.f_ref=tblOtherOut.id and a.StockCode in (select classCode from tblStock where StockFullName like '%'+@condition:StockFullName+'%')))vbCrLf left join tblDepartment on tblDepartment.classCode=tblOtherOut.DepartmentCodevbCrLf left join tblEmployee tblEmployee1 on tblEmployee1.id=tblOtherOut.EmployeeID vbCrLf left join tblEmployee tblEmployee2 on tblEmployee2.id=tblOtherOut.createBy vbCrLf left join tblStock on tblStock.classCode=(case when len(tblOtherOut.StockCode)=0 then tblOtherOutDet.StockCode else tblOtherOut.StockCode end)vbCrLf left join tblBillAccSetup on tblBillAccSetup.SubCode=tblOtherOut.SubCode and tblBillAccSetup.MainType='tblOtherOut'vbCrLf left join tblCompany on tblCompany.classCode=tblOtherOut.CompanyCodevbCrLfwhere 1=1 vbCrLf      and (tblOtherOut.BillDate &gt;= ?)vbCrLf      and (tblOtherOut.BillDate &lt;= ?)vbCrLf      and (tblOtherOut.BillNo like '%'+?+ '%')vbCrLf      and (tblStock.StockFullName like '%'+?+'%')vbCrLf      and (tblEmployee1.EmpFullName like '%'+?+'%')vbCrLf      and (tblDepartment.DeptFullName like '%'+?+'%')vbCrLf      and (tblEmployee2.EmpFullName like '%'+?+'%')vbCrLf      and (tblCompany.ComFullName like '%'+?+'%')vbCrLfvbCrLforder by vbCrLf   tblOtherOut.BillDate desc,vbCrLf   tblOtherOut.BillNo descvbCrLfvbCrLf"/><charts/></ReportDataSource>
