Name as CurrencyName,vbCrLf    k.CurAmount/k.Qty as CurPrice,vbCrLf    k.CurAmount as CurAmount,vbCrLf    k.Discount as Discount,vbCrLf    k.DisBackAmt/k.Qty as DisPrice,vbCrLf    k.DisBackAmt as DisBackAmt,vbCrLf    k.DisAmount as DisAmount,vbCrLf    case when isnull(k.Qty,0)=0 then 0 else isnull(k.TaxAmount,0)/isnull(k.Qty,0) end as TaxPrice,vbCrLf    isnull(k.TaxAmount,0) as TaxAmount,vbCrLf    k.CoTaxAmt as CoTaxAmt,vbCrLf    tblGoods.SCompanyID as SCompanyID,vbCrLf    tblGoods.isCatalog as isCatalog,vbCrLf    isnull(k.Qty,0) as lastQty1,vbCrLf    (case when '@Sess:Local'='zh_CN' then '商品期初明细' when '@Sess:Local'='zh_TW' then '商品期初明細' when '@Sess:Local'='en' then 'Goods Initial Detail' end) as GoodsList,vbCrLf    tblGoods.classCode+'#;#0#;##;#' as hideCode,vbCrLf    k.remark as Mremark,vbCrLf    k.dremark as DremarkvbCrLffrom tblGoods vbCrLfleft join(select b.GoodsCode,c.StockCode,b.BatchNo,b.Inch,b.Hue,b.yearNO,b.ProDate,b.Availably,c.remark,max(b.remark) dremark,SUM(isnull(b.Qty,0)) as Qty,SUM(isnull(b.Amount,0)) as Amount,SUM(isnull(b.TaxAmount,0)) as TaxAmount,b.Discount,sum(DisBackAmt) as DisBackAmt,sum(b.DisAmount) as DisAmount,sum(CoTaxAmt) as CoTaxAmt,sum(CurAmount) as CurAmount,c.Currency,sum(cast(b.UnitIntQty as float)) as UnitIntQty,sum(cast(b.UnitBaseQty as float)) as UnitBaseQty,sum(cast(b.UnitQty as float)) as UnitQty vbCrLf          from tblBeginStockDet b vbCrLf          left join tblBeginStock c on b.f_ref=c.id vbCrLf          where c.workFlowNodeName='finish' group by b.GoodsCode,c.StockCode,b.BatchNo,b.Inch,b.Hue,b.yearNO,b.ProDate,b.Availably,b.Discount,c.Currency,c.remarkvbCrLf) as k on tblGoods.classCode=k.GoodsCodevbCrLfleft join tblStock on tblStock.classCode=k.StockCodevbCrLfleft join tblGoodsunit on tblGoodsunit.GoodsCode=tblGoods.classCode and DefaultAssit=1vbCrLfleft join tblCurrency on tblCurrency.id=k.CurrencyvbCrLf{k.GOODSPROP}vbCrLfwhere 1=1 vbCrLf      and (tblStock.classCode =?)vbCrLf      and (tblGoods.classCode =?)vbCrLf      and tblGoods.isCatalog=0vbCrLf      and (tblCurrency.CurrencyName like '%'+?+'%')vbCrLf      and (tblGoods.SCompanyID like '@Sess:SCompanyID'+'%')vbCrLf      and (isnull(k.Qty,0) &lt;&gt; ?)vbCrLfvbCrLforder by vbCrLf   tblGoods.[tblGoods] vbCrLfvbCrLf"/><charts/></ReportDataSource>
