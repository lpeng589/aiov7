public class DefineChartBean {

	private String chartName ;	/*顶部标题*/
	private String type ;		/*图表类型*/
	private ArrayList<String> xAxis ;	/*X轴显示的字段*/
	private ArrayList<String> yAxis ;	/*Y轴显示的字段*/
	private String xAxisName  ;	/*X轴显示标题*/
	private String yAxisName  ;	/*Y轴显示标题*/
	
	public String getChartName() {
		return chartName;
	}
	public void setChartName(String chartName) {
		this.chartName = chartName;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public ArrayList<String> getXAxis() {
		return xAxis;
	}
	public void setXAxis(ArrayList<String> axis) {
		xAxis = axis;
	}
	public ArrayList<String> getYAxis() {
		return yAxis;
	}
	public void setYAxis(ArrayList<String> axis) {
		yAxis = axis;
	}
	public String getXAxisName() {
		return xAxisName;
	}
	public void setXAxisName(String axisName) {
		xAxisName = axisName;
	}
	public String getYAxisName() {
		return yAxisName;
	}
	public void setYAxisName(String axisName) {
		yAxisName = axisName;
	}
}
