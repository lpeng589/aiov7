ilAmt as RetailAmt,vbCrLf    tblAllot.Remark as Remark1,vbCrLf    tblAllot.printCount as printCount,vbCrLf    tblStocks.lastQty as lastQty,vbCrLf    tblStocks.LastQty+tblStocks.inQtyNotAudit+tblStocks.SInQtyNotAudit-tblStocks.outQtyNotAudit-tblStocks.BOutQtyNotAudit as VirtualQtyvbCrLffrom tblAllot vbCrLf left join tblAllotDet on tblAllotDet.f_ref=tblAllot.idvbCrLf left join tblDepartment on tblDepartment.classCode=tblAllot.DepartmentCodevbCrLf left join tblEmployee tblEmployee1 on tblEmployee1.id=tblAllot.EmployeeID vbCrLf left join tblEmployee tblEmployee2 on tblEmployee2.id=tblAllot.createBy vbCrLf left join tblStock tblStock1 on tblStock1.classCode=(case when len(tblAllotDet.StockCode)=0 then tblAllot.StockCode else tblAllotDet.StockCode end)  vbCrLf left join tblStock tblStock2 on tblStock2.classCode=(case when len(tblAllotDet.InStockCode)=0 then tblAllot.InStockCode else tblAllotDet.InStockCode end)vbCrLf left join tblGoods on tblGoods.classCode=tblAllotDet.GoodsCodevbCrLfleft join tblStocks on tblStocks.GoodsCode=tblGoods.classCode  and tblStocks.StockCode=(case when len(tblAllotDet.StockCode)=0 then tblAllot.StockCode else tblAllotDet.StockCode end) and tblStocks.LastQty&gt;0 and tblStocks.BatchNo =tblAllotDet.BatchNo AND tblStocks.Inch =tblAllotDet.Inch AND tblStocks.Hue =tblAllotDet.Hue AND tblStocks.yearNO =tblAllotDet.yearNO AND tblStocks.Availably =tblAllotDet.Availably AND tblStocks.ProDate =tblAllotDet.ProDatevbCrLf{tblAllotDet.GOODSPROP}vbCrLfwhere 1=1 vbCrLf      and (tblAllot.id =?)vbCrLfvbCrLforder by vbCrLf   tblAllotDet.detOrderNo vbCrLfvbCrLf"/><charts/></ReportDataSource>
