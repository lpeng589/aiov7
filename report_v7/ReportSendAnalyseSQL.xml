n (select SourceID,sum(Qty) as Qty,SUM(CheckQty) as CheckQty,SUM(NotCheckQty) as NotCheckQty from tblSendDet join tblSend on tblSend.id=tblSendDet.f_ref where tblSend.workFlowNodeName='finish' group by SourceID)a on tblBuyApplicationTotalDet.id=a.SourceID where tblBuyApplicationTotal.workFlowNodeName='finish' group by tblBuyApplicationTotalDet.SourceID,tblBuyApplicationTotalDet.SourceID)TotalDet on TotalDet.SourceID=tblBuyApplicationDet.idvbCrLfjoin tblGoods on  tblGoods.classCode=substring(tblBuyApplicationDet.GoodsCode,1,len(tblGoods.classCode)) where tblGoods.isCatalog=1 group by f_ref,tblGoods.classCode,BatchNo,Inch,Hue,yearNO,ProDate,Availably)SendAnalyse on SendAnalyse.f_ref=tblBuyApplication.idvbCrLf left join tblDepartment on tblBuyApplication.DepartmentCode=tblDepartment.classCodevbCrLf join tblGoods on SendAnalyse.GoodsCode=tblGoods.classCodevbCrLf{SendAnalyse.GOODSPROP}vbCrLfwhere 1=1 vbCrLf      and (tblBuyApplication.BillDate &gt;=? and tblBuyApplication.workFlowNodeName='finish')vbCrLf      and (tblBuyApplication.BillDate &lt;=?)vbCrLf      and (tblGoods.classCode =?)vbCrLf      and (tblBuyApplication.DepartmentCode =?)vbCrLf      and (SendAnalyse.[GOODSPROP] =?)vbCrLfgroup by vbCrLf   tblGoods.classCode,vbCrLf   tblBuyApplication.DepartmentCode,vbCrLf   tblGoods.[tblGoods],vbCrLf   SendAnalyse.[GOODSPROP],vbCrLf   tblDepartment.DeptFullName,vbCrLf   tblGoods.classCode,vbCrLf   tblGoods.isCatalogvbCrLfvbCrLfvbCrLf"/><charts/></ReportDataSource>
