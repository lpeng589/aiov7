<?xml version="1.0" encoding="gb2312"?>
<popupSelect>
<select name="freightAcc">
     <table>tblAccTypeInfo</table>
     <forwardModel>tblAccTypeInfo</forwardModel>
     <relation></relation>
     <saveParentFlag></saveParentFlag>
     <defineSQL>select 
      tblAccTypeInfo.AccNumber f1,
      tblAccTypeInfo.AccName f2,
      tblAccTypeInfo.AccFullName f3,
      tblAccTypeInfo.PyCode f4,
	  tblAccTypeInfo.classCode f5
from tblAccTypeInfo
     </defineSQL>
     <classCode>tblAccTypeInfo.classCode</classCode>
     <remark>回填运费类型</remark>
     <condition></condition>
     <changeCond>tblAccTypeInfo.statusId!='-1' and isnull(tblAccTypeInfo.isCalculate,'')!='1' and tblAccTypeInfo.AccNumber not like '1122%' and tblAccTypeInfo.AccNumber not like '2203%'</changeCond>
     <orderBy>tblAccTypeInfo.AccNumber</orderBy>
     <displayFields>
          <field name="tblAccTypeInfo.AccNumber" width="100" searchType="match" keySearch="true" parentDisplay="false" orderBy="升" />
          <field name="tblAccTypeInfo.AccName" width="150" searchType="match" keySearch="true" parentDisplay="true" display="@TABLENAME.freightAcc" />
          <field name="tblAccTypeInfo.AccFullName" width="200" searchType="match" parentDisplay="false" />
          <field name="tblAccTypeInfo.PyCode" width="50" searchType="match" keySearch="true" parentDisplay="false" hiddenInput="true" />
     </displayFields>
     <saveFields>
          <field name="tblAccTypeInfo.AccNumber" parentName="@TABLENAME.freightAcc" relationKey="true" hidden="true" orderBy="升" />
     </saveFields>
</select>
</popupSelect>
